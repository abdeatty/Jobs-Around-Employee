<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragments.profile.ProfileFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:elevation="@dimen/_4sdp"
            android:gravity="center"
            app:boxCornerRadiusBottomEnd="@dimen/_2sdp"
            app:boxCornerRadiusBottomStart="@dimen/_2sdp"
            app:boxCornerRadiusTopEnd="@dimen/_2sdp"
            app:boxCornerRadiusTopStart="@dimen/_2sdp"
            app:layout_constraintTop_toTopOf="parent">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="@color/white"

                android:drawableEnd="@drawable/ic_star"
                android:drawablePadding="@dimen/_10sdp"
                android:focusable="false"
                android:fontFamily="@font/font_regular"
                android:text="وظائفى"
                android:textColor="@color/colorPrimary" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:elevation="@dimen/_4sdp"
            android:gravity="center"
            app:boxCornerRadiusBottomEnd="@dimen/_2sdp"
            app:boxCornerRadiusBottomStart="@dimen/_2sdp"
            app:boxCornerRadiusTopEnd="@dimen/_2sdp"
            app:boxCornerRadiusTopStart="@dimen/_2sdp"
            app:layout_constraintTop_toTopOf="parent">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="@color/white"

                android:drawableEnd="@drawable/ic_star"
                android:drawablePadding="@dimen/_10sdp"
                android:focusable="false"
                android:fontFamily="@font/font_regular"
                android:text="المفضله"
                android:textColor="@color/colorPrimary" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:elevation="@dimen/_4sdp"
            android:gravity="center"
            app:boxCornerRadiusBottomEnd="@dimen/_2sdp"
            app:boxCornerRadiusBottomStart="@dimen/_2sdp"
            app:boxCornerRadiusTopEnd="@dimen/_2sdp"
            app:boxCornerRadiusTopStart="@dimen/_2sdp"
            app:layout_constraintTop_toTopOf="parent">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="@color/white"
                android:drawableEnd="@drawable/ic_star"
                android:drawablePadding="@dimen/_10sdp"
                android:focusable="false"
                android:fontFamily="@font/font_regular"
                android:text="بيانتى"
                android:textColor="@color/colorPrimary" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:elevation="@dimen/_4sdp"
            android:gravity="center"
            app:boxCornerRadiusBottomEnd="@dimen/_2sdp"
            app:boxCornerRadiusBottomStart="@dimen/_2sdp"
            app:boxCornerRadiusTopEnd="@dimen/_2sdp"
            app:boxCornerRadiusTopStart="@dimen/_2sdp"
            app:layout_constraintTop_toTopOf="parent">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTint="@color/white"
                android:drawableEnd="@drawable/ic_star"
                android:drawablePadding="@dimen/_10sdp"
                android:focusable="false"
                android:fontFamily="@font/font_regular"
                android:text="الرسوم"
                android:textColor="@color/colorPrimary" />
        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_20sdp"
        android:fontFamily="@font/font_regular"
        android:gravity="center"
        android:text="تسجيل الخروج"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@id/about_us" />

    <TextView
        android:id="@+id/contact_us_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_30sdp"
        android:fontFamily="@font/font_regular"
        android:text="إتصل بنا"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/about_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_30sdp"
        android:fontFamily="@font/font_regular"
        android:text="عن التطبيق"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>